{
    "name": "razor-vscode-functional-test",
    "private": true,
    "displayName": "Razor Functional Tests",
    "version": "0.0.1",
    "engines": {
        "vscode": "1.45.1"
    },
    "scripts": {
        "vscode:prepublish": "yarn run compile",
        "clean": "rimraf dist",
        "build": "yarn run clean && yarn run lint && tsc -p ./",
        "lint": "eslint --ext .ts ./tests",
        "watch": "yarn run clean && yarn run lint && tsc -watch -p ./",
        "test": "yarn build && node ./dist/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^9.4.7",
        "@types/rimraf": "2.0.2",
        "@types/vscode": "1.45.1",
        "@typescript-eslint/eslint-plugin":"2.31.0",
        "@typescript-eslint/parser": "2.31.0",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "rimraf": "2.6.3",
        "ts-node": "^7.0.1",
        "eslint": "6.8.0",
        "eslint-config-razor-tooling": "link:../../eslint-config-razor-tooling/",
        "eslint-plugin-jsdoc": "22.1.0",
        "eslint-plugin-header": "3.0.0",
        "typescript": "3.8.3",
        "vscode-test": "1.3.0"
    },
    "dependencies": {},
    "eslintConfig": {
      "extends": "eslint-config-razor-tooling"
    }
}
