{
  "name": "razor-vscode-test",
  "private": true,
  "displayName": "Razor Unit Tests",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.1"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && yarn run lint && tsc -p ./",
    "lint": "eslint --ext .ts ./tests",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --colors"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "9.4.7",
    "@types/vscode": "1.45.1",
    "@typescript-eslint/eslint-plugin":"2.31.0",
    "@typescript-eslint/parser": "2.31.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "ts-node": "^7.0.1",
    "eslint": "6.8.0",
    "eslint-plugin-jsdoc": "22.1.0",
    "eslint-plugin-header": "3.0.0",
    "typescript": "3.8.3",
    "rimraf": "2.6.3"
  },
  "dependencies": {
    "@typescript-eslint/parser": "2.31.0",
    "eslint-config-razor-tooling": "link:../../eslint-config-razor-tooling/",
    "microsoft.aspnetcore.razor.vscode": "link:../../src/Microsoft.AspNetCore.Razor.VSCode",
    "vscode-test": "^1.3.0"
  },
  "eslintConfig": {
    "extends": "eslint-config-razor-tooling"
  }
}
